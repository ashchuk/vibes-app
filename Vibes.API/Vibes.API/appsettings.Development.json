{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BotConfiguration": {
    "BotToken": "BOT_TOKEN",
    "BotWebhookUrl": "https://your.app.url/api/bot",
    "SecretToken": "TOP_SECRET"
  },
  "GeminiConfiguration": {
    "ApiKey": "GEMINI_API_KEY"
  },
  "GoogleCalendarConfiguration": {
    "RedirectUri": "https://your.app.url/api/google-auth/callback",
    "ApplicationName": "vibes-app-client",
    "ClientId": "client-id",
    "ClientSecret": "client-secret"
  },
  "MemoConfiguration": {
    "BaseUrl": "https://api.mem0.ai",
    "ApiKey": "your-api-key"
  },
  "QuartzConfiguration": {
    "MorningCheckupCronSchedule": "0 15 3 * * ?",
    "EveningCheckupCronSchedule": "0 0 18 * * ?",
    "Comment": "\"0 15 6 * * ?\" означает \"Каждый день в 6:15:00 утра по UTC\"."
  }
}
